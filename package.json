{
  "name": "gatsby-source-gooten",
  "version": "1.0.0",
  "description": "a gatsby source plugin to get data from gooten.com",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
